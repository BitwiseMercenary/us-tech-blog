{
  "name": "tech-blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "cname": "echo tech.opentable.co.uk > out/CNAME",
    "nojekyll": "touch out/.nojekyll",
    "build": "next build && next export && npm run cname && npm run nojekyll",
    "start": "next start",
    "lint": "next lint",
    "prettier": "prettier --write .",
    "new": "./scripts/new",
    "push": "gh-pages -b public -d out --dotfiles true",
    "deploy": "npm run build && npm run push"
  },
  "dependencies": {
    "gh-pages": "^4.0.0",
    "gray-matter": "^4.0.3",
    "jsdom": "^20.0.0",
    "next": "12.3.0",
    "prismjs": "^1.29.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rehype-prism-plus": "^1.5.0",
    "rehype-stringify": "^9.0.3",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "unified": "^10.1.2"
  },
  "devDependencies": {
    "eslint": "8.11.0",
    "eslint-config-next": "12.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1"
  }
}
